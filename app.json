{
	"expo": {
		"name": "Wifi Clip Controller",
		"slug": "witblox_wifi_clip",
		"version": "2.6.3",
		"jsEngine": "hermes",
		"orientation": "portrait",
		"icon": "./assets/icon.png",
		"userInterfaceStyle": "light",
		"splash": {
			"image": "./assets/splash.png",
			"resizeMode": "cover",
			"backgroundColor": "#ffffff"
		},
		"updates": {
			"fallbackToCacheTimeout": 0
		},
		"assetBundlePatterns": ["**/*"],
		"ios": {
			"supportsTablet": true,
			"bundleIdentifier": "com.witblox.wifiClip",
			"buildNumber": "2.6.3",
			"requireFullScreen": true,
			"jsEngine": "jsc",
			"infoPlist": {
				"NSMicrophoneUsageDescription": "This app uses the microphone to detect speech.",
				"NSSpeechRecognitionUsageDescription": "This app uses speech recognition to control the hardware."
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/adaptive-icon.png",
				"backgroundColor": "#FFFFFF"
			},
			"package": "com.witblox.wifi_clip",
			"versionCode": 263
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"extra": {
			"eas": {
				"projectId": "bae0a76c-2c15-4bd7-af1e-ce116572f7b7"
			}
		},
		"plugins": [
			[
				"expo-build-properties",
				{
					"android": {
						"usesCleartextTraffic": true,
						"targetSdkVersion": 34,
						"maxSdkVersion": 34
					}
				}
			]
		]
	}
}
