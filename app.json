{
  "expo": {
    "name": "witblox_wifi_clip",
    "slug": "witblox_wifi_clip",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/witblox-logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash3.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.witblox.wifiClip"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.witblox.wifi_clip"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}